!--------------------------------------------------
! XServer configuration file
! file:     ~/.Xresources
!--------------------------------------------------

! Cursor settings:
xcursor.theme:			Vanilla-DMZ

! urxvt settings:
urxvt*font:             xft:DejaVu Sans Mono for Powerline:pixelsize=13
urxvt*scrollBar:		false
urxvt*internalBorder:	5
urxvt*letterSpace:      -1
urxvt*cursorBlink:      true
URxvt.perl-ext-common:  default,matcher
URxvt.matcher.button:   1
urxvt.urlLauncher:   	/usr/bin/chromium
urxvt.underlineURLs: 	true
URxvt.colorUL:          #4682B4
urxvt.inputMethod:      none
urxvt.preeditType:      OverTheSpot
urxvt.cursorColor:      #DDDDDD
urxvt*background:       #000000
urxvt*foreground:       #ffffff

! xdvi settings:
xdvi*background:		white
xdvi*foreground:		black
xdvi.editor:            gvim --remote-wait +%l "+normal %c|" %f

! xpdf settings:
xpdf*enableFreetype:    yes
xpdf*background:        white
xpdf*foreground:        black

urxvt*colorMode:          on
urxvt*colorBD:            lightyellow
urxvt*colorUL:            yellow
urxvt*colorULMode:        on
urxvt*dynamicColors:      off
urxvt*highlightSelection: true

urxvt.color0: #000000
